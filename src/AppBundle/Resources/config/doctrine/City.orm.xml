<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\City" table="city">
    <indexes>
      <index name="IDX_2D5B02348C779B08" columns="idcountry"/>
    </indexes>
    <id name="idcity" type="integer" column="idcity">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="city_idcity_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="iso6393" type="string" column="iso639_3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fullname" type="string" column="fullname" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phonecitycode" type="string" column="phonecitycode" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="latitude" type="decimal" column="latitude" precision="18" scale="9" nullable="false"/>
    <field name="longitude" type="decimal" column="longitude" precision="18" scale="9" nullable="false"/>
    <many-to-one field="idcountry" target-entity="Country" fetch="LAZY">
      <join-columns>
        <join-column name="idcountry" referenced-column-name="idcountry"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
