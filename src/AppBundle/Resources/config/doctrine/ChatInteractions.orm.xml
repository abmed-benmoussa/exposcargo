<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ChatInteractions" table="chat_interactions">
    <id name="messageId" type="integer" column="message_id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="chat_interactions_message_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="toId" type="string" column="to_id" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fromId" type="string" column="from_id" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="message" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="time" type="datetime" column="time" nullable="false"/>
    <field name="ipAddress" type="string" column="ip_address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
