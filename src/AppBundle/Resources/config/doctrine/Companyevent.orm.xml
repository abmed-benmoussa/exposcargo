<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Companyevent" table="companyevent">
    <indexes>
      <index name="IDX_25E66052EDAB66BE" columns="idevent"/>
    </indexes>
    <id name="idcompanyevent" type="integer" column="idcompanyevent">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="companyevent_idcompanyevent_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="companyname" type="string" column="companyname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idevent" target-entity="Event" fetch="LAZY">
      <join-columns>
        <join-column name="idevent" referenced-column-name="idevent"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
