<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Procedures" table="procedures">
    <id name="idprocedure" type="integer" column="idprocedure">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="procedures_idprocedure_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comment" type="string" column="comment" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="authrequired" type="integer" column="authrequired" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
